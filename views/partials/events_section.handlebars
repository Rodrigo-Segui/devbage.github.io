<section id="event" class="event-mf sect-pt4 route">
<div id="carousel" class="my-5">
  <div id="carousel-items" class="row">
    {{#each eventos}}
      {{!-- Product --}}
      <div class="col d-flex flex-column justify-content-center align-items-center mx-4">

        <p style="color: green;" class="prod-name text-center my-0 px-1">{{dataValues.nome}}</p>

        <div class="prod-description d-flex justify-content-center align-items-center">
          <p class="text-center my-0 px-1">
            {{dataValues.descricao}}
          </p>
        </div>

        <div class="w-100 text-center mt-1">
          <a href="/produtos/cadastrar">
            <button class="btn btn-dk px-2 px-md-4 py-1">
              <i class="fas fa-shopping-cart"></i>
                    Clique Aqui para se Inscrever</button>
          </a>
        </div>

      </div> 
    {{/each}}
  </div>

  <div id="btn-prev" class="d-inline">
    <div class="d-flex h-100 align-items-center justify-content-end">
      <i class="fas fa-angle-left fa-2x"></i>
    </div>
  </div>

  <div id="btn-next" class="d-inline">
    <div class="d-flex h-100 align-items-center justify-content-start">
      <i class="fas fa-angle-right fa-2x"></i>
    </div>
  </div>
</div>
</section>